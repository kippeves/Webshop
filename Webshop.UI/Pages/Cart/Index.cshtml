@page "{handler?}/{id?}"
@model Webshop.UI.Pages.Cart.IndexModel
@{
}
<h1>Produkter i varukorg:</h1>
<table class="table">
    <thead class="thead-dark">
        <tr>
            <th scope="col">#</th>
            <th scope="col">Namn</th>
            <th scope="col">Mängd</th>
        </tr>
    </thead>
    <tbody>
        @for (int i = 0; i < Model.Products.Count; i++)
        {
            <tr>
                <th scope="row">@i</th>
                <td>@Model.Products.ToArray()[i].Key.Name</td>
                <td>@Model.Products.ToArray()[i].Value</td>
            </tr>
        }
    </tbody>
</table>
